{
    "name": "ghul",
    "displayName": "ghūl",
    "description": "ghūl language support",
    "version": "0.0.11",
    "publisher": "degory",
    "engines": {
        "vscode": "^1.16.1"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ghul",
            "aliases": ["ghul", "ghul"],
            "extensions": [".ghul"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ghul",
            "scopeName": "source.ghul",
            "path": "./syntaxes/ghul.tmLanguage.json"
        }],
        "snippets": [{
            "language": "ghul",
            "path": "./snippets/ghul.json"
        }]
    }
}
