{
	"files.exclude": {
		"**/*.js": true,
		"**/*.js.map": true,
		"**/node_modules": true,
		"out": true
	},
	"search.exclude": {
		"out": true, // set this to false to include "out" folder in search results
		"**/node_modules": true,
		"**/*.js": true,
		"**/*.js.map": true
	},
	"typescript.tsdk": "./node_modules/typescript/lib",
	"typescript.tsc.autoDetect": "off"
}
